<mjml>
  <mj-head>
    <mj-attributes>
      <mj-all padding="0px"></mj-all>
      <mj-class name="preheader" color="#000000" font-size="11px" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding="0px"></mj-class>
    </mj-attributes>
    <mj-style inline="inline">
      a {
        text-decoration: none;
        color: inherit;
      }

      img {
        color: "#fff";
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#3b3b3b">
    <mj-section background-color="#242424" padding="10px 0" border-bottom="solid 1px #424242">
      <mj-column width="33%">
        <mj-image src="data:image/png;base64," alt="{{APP_NAME}}" width="150px" padding="10px 0"></mj-image>
      </mj-column>
      <mj-column width="66%">
      </mj-column>
    </mj-section>
    <mj-section background-color="#242424" padding-top="20px">
      <mj-column width="100%">
        <mj-text align="center" color="#0c8599" font-size="25px" font-family="Lato, Helvetica, Arial, sans-serif" padding="10px 25px"><strong>Hey {{name}}
            <br />
            <br />
            {{title}}
          </strong></mj-text>
        <mj-text align="center" color="#fff" font-size="18px" font-family="Lato, Helvetica, Arial, sans-serif" padding="10px 30px"><strong>{{message}}</strong></mj-text>
      </mj-column>
    </mj-section>
    <mj-section background-color="#242424">
  <mj-column width="100%">
    <mj-button href="{{passwordResetUrl}}" align="center" background-color="#0c8599" color="#FFFFFF" border-radius="2px" href="#" inner-padding="15px 30px" padding-bottom="20px" padding-top="20px">
      Reset Password Now
    </mj-button>
  </mj-column>
</mj-section>
    <mj-section background-color="#242424" padding="10px" border-top="solid 1px #424242">
      <mj-column vertical-align="top" width="100%">
        <mj-text align="center" color="#fff" font-size="20px" font-family="Lato, Helvetica, Arial, sans-serif" padding="10px 25px">Best, <br /><br /> The {{APP_NAME}} Team</mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>